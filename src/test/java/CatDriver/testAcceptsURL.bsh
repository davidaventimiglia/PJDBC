import org.pjdbc.drivers.*;
getClass("org.pjdbc.drivers.MockDriver");
assertFalse(new CatDriver().acceptsURL("jdbc:cat"));
assertFalse(new CatDriver().acceptsURL("jdbc:cat:"));
